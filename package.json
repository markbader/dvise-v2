{
  "name": "dvise-v2",
  "displayName": "dvise.v2",
  "description": "Digital Mentor for Visualizing Software Engineering",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dviseContainer",
          "title": "Dvise",
          "icon": "resources/dvise-icon.svg"
        }
      ]
    },
    "views": {
      "dviseContainer": [
        {
          "id": "dvise.fileExplorer",
          "name": "Dvise File Explorer",
          "icon": "resources/dvise-icon.svg"
        },
        {
          "id": "dvise.chatView",
          "name": "Chat Assistant",
          "icon": "resources/dvise-icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "dvise.setApiKey",
        "title": "Set OpenAI API Key"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/d3-selection": "^3.0.11",
    "@types/mermaid": "^9.2.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.13.16",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "marked": "^15.0.7",
    "mermaid": "^11.6.0",
    "type-fest": "^4.39.0"
  }
}